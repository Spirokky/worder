{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "node": true,
    "mongo": true,
    "mocha": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "linebreak-style": 0,
    "require-jsdoc": 0,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "func-names": 0
  }
}
